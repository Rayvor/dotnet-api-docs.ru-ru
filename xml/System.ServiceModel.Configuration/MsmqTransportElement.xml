      <MemberSignature Language="F#" Value="member this.UseActiveDirectory : bool with get, set" Usage="System.ServiceModel.Configuration.MsmqTransportElement.UseActiveDirectory" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
